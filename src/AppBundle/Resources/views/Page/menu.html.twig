{% if menu is not empty %}
    <header>
        <h1>{{ menu.header }}</h1>
    </header>

    <ul>
        {% for menu_item in menu.items %}
            <li><a href="{{ menu_item.url }}">{{ menu_item.title }}</a></li>
        {% endfor %}
    </ul>

    <footer>
        <a href="#">&gt;&gt;&hellip;</a>
    </footer>
{% endif %}
