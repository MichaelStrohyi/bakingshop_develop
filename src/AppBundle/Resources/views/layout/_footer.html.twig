{#<div class="footer-top"></div>#}
<section class="footer-bottom">
    {% for menu in menus %}
        {% if menu.type == constant('AppBundle\\Entity\\Menu::TYPE_BOTTOM') %}
            <div class="footer-menu">
                <div class="menu-title"><span>{{ menu.header }}</span></div>
                <nav class="menu-items">
                    <ul class="menu-list">
                        {{ render(controller('AppBundle:Page:menu', {'name': menu.name})) }}
                    </ul>
                </nav>
            </div>
        {% endif %}
    {% endfor %}
</section>